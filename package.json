{
	"name": "vscode-civvi-environment",
	"displayName": "Civilization VI Environment Emulation",
	"publisher": "WildW",
	"icon": "images/logoSquare.png",
	"description": "Autocomplete for Civilization VI modding (currently only lua)",
	"version": "1.1.0",
	"keywords": [
		"EmmyLua",
		"IntelliSense",
		"lua",
		"Civ",
		"Civilization",
		"Modding"
	],
	"license": "MIT",
	"engines": {
		"vscode": "^1.58.0"
	},
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	"activationEvents": [
		"onLanguage:lua"
	],
	"main": "./out/extension.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/Wild-W/vscode-civVI-environment-master"
	},
	"extensionDependencies": [
		"sumneko.lua"
	],
	"scripts": {
		"patch": "vsce publish patch && git push",
		"minor": "vsce publish minor && git push",
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.58.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
